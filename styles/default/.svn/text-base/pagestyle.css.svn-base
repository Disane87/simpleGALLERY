@charset "utf-8";

body {
	background-color: #0080c0;
	font-family:verdana;
	font-size:10px;
	color:white;

	background-color:black;
	padding: 0;
	
	scrollbar-face-color: #000000;
	scrollbar-highlight-color: #000000;
	scrollbar-3dlight-color: #000000;
	scrollbar-darkshadow-color: #000000;
	scrollbar-shadow-color: #72C723;
	scrollbar-arrow-color: #72C723;
	scrollbar-track-color: #000000;
}

label
{
	margin:0px 0px 0px 5px;	
	color:white;
	border-bottom:1px dotted #72C723;
	cursor:help;
}
fieldset
{
	padding:5px 5px 5px 5px;
	border:1px dotted #72C723;
}
legend
{
	padding:0 5px 0 5px;
	border-left:1px dotted #72C723;
	border-right:1px dotted #72C723;
	
}
checkbox
{
	border:1px dotted #72C723;
}
#header
{
	position:absolute;
	border-bottom:1px #72C723 solid;
	border-left:1px #72C723 solid;
	color:white;
	font-weight:bold;

	margin-left:5px;
	padding-left:5px;
	text-align:left;
	left:5px;
	right:5px;
	width:auto;
	top:5px;

}
#header [type=button]{
	border-bottom:1px #72C723 dotted;
	border-top:none;
	border-left:none;
	border-right:none;
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	color:white;
	cursor:pointer;
}
#footer
{
	border-top:1px #72C723 solid;
	color:white;
	text-align:left;
	position:absolute;
	bottom:0px;
	width:auto;
	margin-bottom:5px;
	margin-left:5px;
	padding-left:5px;
	left:5px;
	right:5px;
}
#pageing
{
	color:white;
	text-align:center;
	position:absolute;
	bottom:0px;
	width:auto;
	margin-bottom:5px;
	margin-left:5px;
	padding-left:5px;
	left:5px;
	right:5px;
}
#back2Uebersicht a:link, #back2Uebersicht a:visited
{
	border-bottom:1px dotted #72C723;
}

#content {
	width: auto;
	background-color:black;
	vertical-align:top;
	/*border:1px dashed #72C723;*/
	left:15px;
	right:15px;
	position:absolute;
	bottom:25px;
	top:40px;
	height:auto;
	padding:15px;	
	overflow-x:auto;
	text-align:center;
}
#content [type=text], [type=password], select, TEXTAREA{
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	color:white;
	width:200px;
	text-align:left;
	border:1px #72C723 dashed;
}
#Settings [type=text], select{
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	color:white;
	width:200px;
	text-align:left;
	border-top:none;
	border-left:none;
	border-right:none;
	border-bottom:1px dotted 72C723;
}
#login [type=text], select{
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	color:white;
	width:200px;
	text-align:left;
	border:1px #72C723 dashed;
}

#txtSuche{
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	color:white;
	width:150px;
	text-align:center;
	/*border:none;*/
	border:1px #72C723 dotted;
}
#content [type=file]{
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	width:200px;
	/*border:none;*/
	border:1px #72C723 solid;
}
#content [type=submit], #content [type=reset], #content [type=button]{
	border:1px #72C723 solid;
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	color:white;
}
#login [type=submit], #login [type=reset], #login [type=button]{
	border:1px #72C723 solid;
	background-color:transparent;
	font-family:verdana;
	font-size:10px;
	color:white;
}

#content A:link, #content A:visited { 
	color: white; 
	background-color:transparent; 
	text-decoration:none;
}
#content A:hover, A:focus  {  
	color: #72C723; 
	text-decoration:none;
	background-color:transparent;
}

#error
{
	text-align:left;
	border:solid 1px #FF6B6B;
	background-color:#FFE5E5;
	padding:5px;
	color:black;
	position:absolute; 
	bottom: 25px; 
	width: 97%; 	
}
#tabledescripton
{
	border-bottom:1px #72C723 solid;
}
#PictureFrame
{
	border:1px #72C723 dotted;
	padding:15px;

	vertical-align:top;
	text-align:center;
}

#Picture:hover
{
	alpha(opacity=100);
	-moz-opacity:1.0;
	opacity:1.0;
}
#Picture
{
	alpha(opacity=75);
	-moz-opacity:.75;
	opacity:.75;
}
#markedItem
{
	color: #72C723;
	border-bottom:1px #72C723 dotted;
	font-weight:bold;
}
.GalleryHover
{
	color:#72C723;
	cursor:default;
}
img{
 border:none;
 margin:0px 0px 0px 0px;
 padding: 0px;
}

h2
{
	width:100%;
	color: white;
	border-bottom:1px #72C723 dotted;
	font-weight:bold;
}
#tabs
{
	color:white;
}
#tabs a:link, #tabs a:visited 
{
	background-color:transparent;
	color:white;
	text-decoration:none;
}

/* THICK-BOX Anfang */
/* ----------------------------------------------------------------------------------------------------------------*/
/* ---------->>> global settings needed for thickbox <<<-----------------------------------------------------------*/
/* ----------------------------------------------------------------------------------------------------------------*/
*{padding: 0; margin: 0;}

/* ----------------------------------------------------------------------------------------------------------------*/
/* ---------->>> thickbox specific link and font settings <<<------------------------------------------------------*/
/* ----------------------------------------------------------------------------------------------------------------*/
#TB_window {
	font: 11px Verdana;
	color: white;
}

#TB_secondLine {
	font: 11px Verdana;
	color:white;
}

#TB_window a:link {color: white;}
#TB_window a:visited {color: white;}
#TB_window a:hover {color: #72C723;}
#TB_window a:active {color: white;}
#TB_window a:focus{color: white;}

/* ----------------------------------------------------------------------------------------------------------------*/
/* ---------->>> thickbox settings <<<-----------------------------------------------------------------------------*/
/* ----------------------------------------------------------------------------------------------------------------*/
#TB_overlay {
	position: fixed;
	z-index:100;
	top: 0px;
	left: 0px;
	height:100%;
	width:100%;
}

.TB_overlayMacFFBGHack {background: url(macFFBgHack.png) repeat;}
.TB_overlayBG {
	background-color:#000;
	filter:alpha(opacity=75);
	-moz-opacity: 0.75;
	opacity: 0.75;
}

* html #TB_overlay { /* ie6 hack */
     position: absolute;
     height: expression(document.body.scrollHeight > document.body.offsetHeight ? document.body.scrollHeight : document.body.offsetHeight + 'px');
}

#TB_window {
	position: fixed;
	background: #000000;
	z-index: 102;
	color:#000000;
	display:none;
	border: 1px solid #72C723;
	text-align:left;
	top:50%;
	left:50%;
}

* html #TB_window { /* ie6 hack */
position: absolute;
margin-top: expression(0 - parseInt(this.offsetHeight / 2) + (TBWindowMargin = document.documentElement && document.documentElement.scrollTop || document.body.scrollTop) + 'px');
}

#TB_window img#TB_Image {
	display:block;
	margin: 15px 0 0 15px;

}

#TB_caption{
	height:25px;
	padding:7px 30px 10px 25px;
	float:left;
	color:white;
}

#TB_closeWindow{
	height:25px;
	padding:11px 25px 10px 0;
	float:right;
	color:white;
}

#TB_closeAjaxWindow{
	padding:7px 10px 5px 0;
	margin-bottom:1px;
	text-align:right;
	float:right;
}

#TB_ajaxWindowTitle{
	float:left;
	padding:7px 0 5px 10px;
	margin-bottom:1px;
}

#TB_title{
	background-color:#e8e8e8;
	height:27px;
}

#TB_ajaxContent{
	clear:both;
	padding:2px 15px 15px 15px;
	overflow:auto;
	text-align:left;
	line-height:1.4em;
}

#TB_ajaxContent.TB_modal{
	padding:15px;
}

#TB_ajaxContent p{
	padding:5px 0px 5px 0px;
}

#TB_load{
	position: fixed;
	display:none;
	height:13px;
	width:208px;
	z-index:103;
	top: 50%;
	left: 50%;
	margin: -6px 0 0 -104px; /* -height/2 0 0 -width/2 */
}

* html #TB_load { /* ie6 hack */
position: absolute;
margin-top: expression(0 - parseInt(this.offsetHeight / 2) + (TBWindowMargin = document.documentElement && document.documentElement.scrollTop || document.body.scrollTop) + 'px');
}

#TB_HideSelect{
	z-index:99;
	position:fixed;
	top: 0;
	left: 0;
	background-color:#fff;
	border:none;
	filter:alpha(opacity=0);
	-moz-opacity: 0;
	opacity: 0;
	height:100%;
	width:100%;
}

* html #TB_HideSelect { /* ie6 hack */
     position: absolute;
     height: expression(document.body.scrollHeight > document.body.offsetHeight ? document.body.scrollHeight : document.body.offsetHeight + 'px');
}

#TB_iframeContent{
	clear:both;
	border:none;
	margin-bottom:-1px;
	margin-top:1px;
	_margin-bottom:1px;
}

/* THICK-BOX ENDE */